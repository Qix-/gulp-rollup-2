{
    "name": "gulp-rollup-2",
    "version": "1.1.7",
    "description": "Gulp plugin for Rollup JavaScript Module Bundler",
    "main": "main.js",
    "scripts": {
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "lint": "eslint main.js -o error.log",
        "mocha": "nyc mocha --async-only main.js",
        "test": "npm run lint && npm run mocha"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/orcunsaltik/gulp-rollup-2.git"
    },
    "keywords": [
        "gulp",
        "rollup",
        "module",
        "modules",
        "bundler",
        "umd",
        "amd",
        "esm",
        "cjs",
        "iife",
        "plugin",
        "gulpplugin",
        "gulp-plugin",
        "javascript",
        "js",
        "es6"
    ],
    "dependencies": {
        "rollup": "^2.29.0",
        "through2": "^4.0.2",
        "vinyl": "^2.2.1",
        "vinyl-sourcemaps-apply": "^0.2.1",
        "object-hash": "^2.0.3",
        "njfs": "^1.2.3"
    },
    "engines": {
        "node": ">= 10.22.1"
    },
    "author": "Orçun Saltık <saltikorcun@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/orcunsaltik/gulp-rollup-2/issues"
    },
    "homepage": "https://github.com/orcunsaltik/gulp-rollup-2#readme",
    "devDependencies": {
        "coveralls": "^3.1.0",
        "eslint": "^7.10.0",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0"
    },
    "eslintConfig": {
        "env": {
            "browser": false,
            "node": true,
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 2018
        }
    }
}
